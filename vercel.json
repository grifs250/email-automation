{
  "functions": {
    "api/*.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "regions": ["arn1"],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Connection",
          "value": "keep-alive"
        }
      ]
    }
  ],
  "edge": {
    "api/submit.edge.js": {
      "memory": 128
    }
  }
} 